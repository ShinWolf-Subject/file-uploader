# Copy this file to .env and modify the values
# .env file is loaded at startup

# API Keys - you can have multiple keys
# Format: API_KEY, API_KEY_1, API_KEY_2, etc.
API_KEY=your-primary-api-key-here
API_KEY_1=backup-key-1
API_KEY_2=backup-key-2

# Server port (default: 7860)
PORT=7860

# Add more keys as needed:
# API_KEY_3=client-1-key
# API_KEY_4=client-2-key
# API_KEY_5=temporary-key
